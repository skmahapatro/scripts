[alias]
    co = checkout
    br = branch
    st = status
    re = reset HEAD --hard
    ci = commit -s
    ca = commit --amend --no-edit
    d  = diff
    d1 = diff HEAD~1
    logline = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    tagline = tag --sort=-taggerdate
[core]
    excludesfile = ~/.gitignore
    editor = 'vim'

